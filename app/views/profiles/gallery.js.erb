$("[data-id='listings']").append("<%= j render(partial: "profiles/picture", collection: @pictures) %>");
$("[data-id='right']").html("<%= j render('shared/gads/right') %>");
$('.placeholder').remove()
$('[data-action="touchTouch"]').touchTouch();

<% if @pictures.next_page %>
  $(".pagination").replaceWith("<%= j(paginate @pictures) %>")
<% else  %>
  $(".pagination").remove();
<% end %>

