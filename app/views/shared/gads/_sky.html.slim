.sky style="height:600px;" data-id="sky-outer"
  - if ENV["SHOW_GADS"]
    ins[class="adsbygoogle margin-bottom-10 pull-right"
         style="display:inline-block;width:300px;height:600px"
         data-ad-client="ca-pub-1350854790289606"
         data-ad-slot="7034568570"
         data-id="sky-ins"]
    javascript:
      (adsbygoogle = window.adsbygoogle || []).push({});

      // if adblock persent remove parent div form flow
      if(!$("[data-id='sky-ins']:visible").length) {
        $("[data-id='sky-outer']").remove();
      }

  - else
    ins.green-bg.margin-bottom-10.pull-right style="display:inline-block;width:300px;height:600px"
