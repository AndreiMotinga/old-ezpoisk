<% unless current_user.try(:team_member?)  %>
  ga('send', 'pageview');
<% end %>

$("[data-id='leaderboard']").remove();
$("[data-id='listings']").append("<%= j render('shared/gads/leaderboard') %>");
$("[data-id='listings']").append("<%= j render(records) %>");
$("[data-id='right']").html("<%= j render('partners/right') %>");
$("[data-id='left']").html("<%= j render('partners/left') %>");

<% if records.next_page %>
  $(".pagination").replaceWith("<%= j(paginate records) %>")
<% else  %>
  $(".pagination").remove();
<% end %>

incrementPartners();
