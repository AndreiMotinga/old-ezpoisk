// send stats
<% if ENV["SEGMENT_KEY"] %>
  ga('send', 'pageview');
<% end %>

// render records
$("[data-id='listings']").append("<%= j render(records) %>");

// update pagination links
<% if records.next_page %>
  $(".pagination").replaceWith("<%= j(paginate records) %>")
<% else  %>
  $(".pagination").remove();
  $("#p2").remove();
<% end %>
