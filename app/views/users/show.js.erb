// send stats
<% if ENV["SEGMENT_KEY"] %>
  ga('send', 'pageview');
<% end %>

// render records
$("[data-id='listings']:visible").append("<%= j render(@records) %>");

// update pagination links
<% if @records.next_page %>
  $(".pagination:visible").replaceWith("<%= j(paginate @records, param_name: @param) %>")
<% else  %>
  $(".pagination:visible").remove();
  $("[data-id='downloading']").remove();
<% end %>
